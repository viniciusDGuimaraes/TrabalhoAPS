package model;

import infrastructure.Database;

public class Singleton {
	private static Singleton instance;
	
	public static Singleton getInstance(){
		if(instance == null){
			instance = new Singleton();
		}
		return instance;
	}
	
	public void reservarLivro(Reserva r){
		Database.class.
	}
	
	private Singleton(){
	}
}
